{
	int a;
	float b;
	A(int i,float x)
	{
		a = i;
		b = x;
	}
	int add(int j)
	{
		return a+j;
	}
}
class B inherits A
{
	List x;
	B(int i,float f, List a)
	{
		A(i,f);
		x = a;
	}
