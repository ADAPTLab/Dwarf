//Constructor overloading
class obj
{
	int a;
	float b;
	string c;
	obj(int x)
	{
		a = x;
		b = 0.0;
		c = "";
	}
	obj(int x,float f)
	{
		a = x;
		b = f;
		c = "";
	}
	obj(int x,string s)
	{
		a = x;
		b = 0.0;
		c = s;
	}
	obj(int x,string s,float f)
	{
		a = x;
		b = f;
		c = s;
	}
}
main()
{	
	 obj x1 = obj(1);
	 obj x2 = obj(2,1.0);
	 obj x3 = obj(4,"yo");
	 obj x4 = obj(5,"hi",2.0);
	 if(x1.a == 1&&x1.b == 0.0&&x1.c == ""&&x2.a == 2&&x2.b == 1.0&&x3.a == 4&&x3.b == 0.0&&x3.c == "yo"&&x4.a == 5&&x4.b == 2.0&&x4.c == "hi")
	{
		write("Test Successful\n");
	}
	else
	{
		write("Test Unsuccessful\n");
	}
}
